{
  "name": "@histoire/plugin-nuxt",
  "version": "0.15.9",
  "description": "Histoire plugin to integrate with Nuxt 3",
  "license": "MIT",
  "author": {
    "name": "Guillaume Chau"
  },
  "repository": {
