  },
  "devDependencies": {
    "@histoire/plugin-svelte": "workspace:*",
    "@sveltejs/vite-plugin-svelte": "^2.0.0",
    "cypress": "^10.8.0",
    "histoire": "workspace:*",
    "start-server-and-test": "^1.14.0",
    "svelte-preprocess": "^4.10.7",
    "vite": "^4.4.8"
  }
}
